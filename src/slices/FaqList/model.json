{
    "type": "SharedSlice",
    "id": "faq_list",
    "name": "FaqList",
    "variations": [
        {
            "id": "default",
            "name": "Default",
            "description": "Standard FAQ list where one answer can be expanded at a time.",
            "version": "initial",
            "imageUrl": "",
            "docURL": "...",
            "primary": {
                "title": {
                    "type": "StructuredText",
                    "config": {
