                                "type": "Text",
                                "config": {
                                    "label": "Question"
                                }
                            },
                            "answer": {
                                "type": "StructuredText",
                                "config": {
                                    "multi": "paragraph,strong,em,hyperlink",
                                    "label": "Answer"
                                }
                            }
                        }
                    }
                }
            }
        }
    ]
}
